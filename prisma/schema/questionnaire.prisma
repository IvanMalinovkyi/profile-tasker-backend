model Questionnaire {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")

  name        String
  profession  String
  description String

  user   User   @relation(fields: [userId], references: [id])
  userId String @unique @map("user_id")

  @@map("questionnaire")
}
